{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "noImplicitUseStrict": true,
    "moduleResolution": "node",
    "types": ["typescript-to-lua/language-extensions", "lua-types/jit"],
    "outDir": "./output",
  },
  "tstl": {
    "luaTarget": "JIT",
    "noHeader": true,
    "noImplicitSelf": true,
    "luaLibImport": "inline"
  }
}